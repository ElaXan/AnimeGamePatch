binary=/data/data/com.termux/files/usr/bin/zex
versionBashIn1="0.5"
updateNotif="1. Add some code"
updateNote="There is new version to"
link=null
printLatest="This is Latest Version (Ver : $versionBashIn1)"
whatTheFuckEditVersion="Something wrong with version.\nThis version different from the server\n\nYou Edit it?"
updateScript1() {
    command cd
    rm $binary
    wget https://raw.githubusercontent.com/ElaXan/AnimeGamePatch/main/zinstall
    mv zinstall $binary
    command clear
    echo "Please run zex again!"
}
