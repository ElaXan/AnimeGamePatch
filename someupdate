binary=/data/data/com.termux/files/usr/bin/zex
versionBashIn1="1.6"
updateNotif="1. Add server Yuuki in change domain function\n2. Fix Code"
updateNote="There is new version to"
link=null
printLatest="This is Latest Version (Ver : $versionBashIn1)"
whatTheFuckEditVersion="Something wrong with version.\nThis version different from the server\n\nYou Edit it?"
updateScript1() {
    command cd
    updateDir=$HOME/updatezex
    scriptUpdate=$updateDir/zinstall
    rm $binary
    mkdir $updateDir
    cd $updateDir
    echo "Update Script!"
    git clone https://github.com/ElaXan/AnimeGamePatch.git &> /dev/null
    if [[ $? != 0 ]]; then
       echo "Can't do update Script. Make sure you internet is ON or stable"
       rm $updateDir
       command cd
       exit
    else
       cd AnimeGamePatch
       mv zex.sh $binary
       command cd
       rm -rf $updateDir
       command clear
       echo "Update success!"
       sleep 1s
       echo "Settings permission!"
       chmod +x $binary
       sleep 1s
       echo "Done!"
       sleep 1s
       command clear
       echo "Please enter command zex again!"
       exit
    fi
}
